{
  "name": "AzureBlobLocation",
  "properties": {
    "published": false,
    "type": "AzureBlob",
    "linkedServiceName": "AzureStorageLinkedService",
    "typeProperties": {
      "folderPath": "testowy/{Year}/{Month}/{Day}.",
      "format": {
        "type": "TextFormat",
        "columnDelimiter": "\t",
        "firstRowAsHeader": true
      },
      "partitionedBy": [
        {
          "name": "Year",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "yyyy"
          }
        },
        {
          "name": "Month",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "%M"
          }
        },
        {
          "name": "Day",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "%d"
          }
        }
      ]
    },
    "availability": {
      "frequency": "Minute",
      "interval": 15
    }
  },
  "$schema": "http://datafactories.schema.management.azure.com/schemas/2015-09-01/Microsoft.DataFactory.Table.json"
}